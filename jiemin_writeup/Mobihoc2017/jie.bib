@article{Sweeney02,
    abstract = {Consider a data holder, such as a hospital or a bank, that has a privately held collection of person-specific, field structured data. Suppose the data holder wants to share a version of the data with researchers. How can a data holder release a version of its private data with scientific guarantees that the individuals who are the subjects of the data cannot be re-identified while the data remain practically useful? The solution provided in this paper includes a formal protection model named k-anonymity and a set of accompanying policies for deployment. A release provides k-anonymity protection if the information for each person contained in the release cannot be distinguished from at least k-1 individuals whose information also appears in the release. This paper also examines re-identification attacks that can be realized on releases that adhere to k- anonymity unless accompanying policies are respected. The k-anonymity protection model is important because it forms the basis on which the real-world systems known as Datafly, {-Argus} and {k-Similar} provide guarantees of privacy protection.},
    address = {River Edge, NJ, USA},
    author = {Sweeney, Latanya},
    citeulike-article-id = {9245921},
    citeulike-linkout-0 = {http://portal.acm.org/citation.cfm?id=774544.774552},
    issn = {0218-4885},
    journal = {Int. J. Uncertain. Fuzziness Knowl.-Based Syst.},
    keywords = {k-anonymity},
    month = oct,
    pages = {557--570},
    posted-at = {2011-05-03 18:55:12},
    priority = {2},
    publisher = {World Scientific Publishing Co., Inc.},
    title = {k-anonymity: a model for protecting privacy},
    url = {http://portal.acm.org/citation.cfm?id=774544.774552},
    volume = {10},
    year = {2002}
}
